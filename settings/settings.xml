<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <pluginGroups>
        <pluginGroup>org.efaps.maven</pluginGroup>
    </pluginGroups>

    <servers>
        <server>
            <id>sonatype-nexus-snapshots</id>
            <username>${env.USER_ID}</username>
            <password>${env.USER_PWD}</password>
        </server>
    </servers>

    <profiles>
        <profile>
            <id>efaps</id>
            <properties>
                 <org.efaps.db.factory>FACTORY</org.efaps.db.factory>
                 <org.efaps.db.type>TYPE</org.efaps.db.type>
                 <org.efaps.db.connection>CONNECTION</org.efaps.db.connection>
            </properties>
            <repositories>
                <repository>
                    <id>central</id>
                    <name>Central Repository</name>
                    <url>https://repo.maven.apache.org/maven2</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <releases>
                        <updatePolicy>never</updatePolicy>
                    </releases>
                </repository>
                <repository>
                    <id>jockihendry</id>
                    <url>https://dl.bintray.com/jockihendry/maven</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>oss_release</id>
                    <url>https://oss.sonatype.org/content/groups/public</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>oss_snapshot</id>
                    <url>https://oss.sonatype.org/content/repositories/snapshots</url>
                    <layout>default</layout>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>maven2-repository.dev.java.net</id>
                    <name>Java.net Repository for Maven</name>
                    <url>https://download.java.net/maven/2/</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>eviwarePluginRepository</id>
                    <url>https://www.soapui.org/repository/maven2/</url>
                    <snapshots>
                        <enabled>false</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                </repository>
                <repository>
                    <id>jboss.org</id>
                    <url>https://repository.jboss.org/maven2/</url>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>jfrog-artifactory</id>
                    <url>https://repo.jfrog.org/artifactory/repo</url>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <!-- testng -->
                <repository>
                    <id>jcenter</id>
                    <name>bintray</name>
                    <url>https://jcenter.bintray.com</url>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
		<repository> 
                     <id>repository.spring.milestone</id> 
                     <name>Spring Milestone Repository</name> 
                     <url>https://repo.spring.io/milestone</url>
	             <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <!-- JavaX classes 
                <repository>
                    <id>ops4j</id>
                    <name>ops4j</name>
                    <url>http://repository.ops4j.org/maven2/</url>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
		-->
            </repositories>
            <pluginRepositories>
                 <pluginRepository>
                    <id>central</id>
                    <name>Central Repository</name>
                    <url>https://repo.maven.apache.org/maven2</url>
                    <layout>default</layout>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <releases>
                        <updatePolicy>never</updatePolicy>
                    </releases>
                </pluginRepository>
                <pluginRepository>
                    <id>snapshot</id>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                    <url>https://oss.sonatype.org/content/groups/public</url>
                    <layout>default</layout>
                </pluginRepository>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                    <id>maven2-repository.dev.java.net</id>
                    <name>Java.net Repository for Maven</name>
                    <url>https://maven.java.net/content/groups/public/</url>
                    <layout>default</layout>
                </pluginRepository>
                <pluginRepository>
                    <id>sonatype-snapshot</id>
                    <name>sonatype-snapshot</name>
                    <url>http://oss.sonatype.org/content/groups/public</url>
                    <layout>default</layout>
                </pluginRepository>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </snapshots>
                    <id>jfrog-plugins</id>
                    <name>jfrog-plugins-dist</name>
                    <url>https://repo.jfrog.org/artifactory/repo</url>
                </pluginRepository>
		<pluginRepository> 
                     <id>repository.spring.milestone-plugins</id> 
                     <name>Spring Milestone Repository</name> 
                     <url>https://repo.spring.io/milestone</url>
	             <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <!-- JavaX classes 
                <pluginRepository>
                    <id>ops4j-plugin</id>
                    <name>ops4j</name>
                    <url>http://repository.ops4j.org/maven2/</url>
                    <releases>
                        <enabled>true</enabled>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
		-->
            </pluginRepositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>efaps</activeProfile>
    </activeProfiles>
</settings>
